{
  "name": "projectname",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --open",
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "test": "vue-cli-service test --testMatch='<rootDir>/src/**.spec.js'",
    "e2e": "vue-cli-service e2e",
    "lint": "vue-cli-service lint",
    "e2e:open": "vue-cli-service e2e:open"
  },
  "dependencies": {
    "register-service-worker": "^1.7.0",
    "vue": "^2.6.11",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.6",
    "@vue/cli-plugin-e2e-cypress": "^3.0.0-beta.6",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.6",
    "@vue/cli-plugin-pwa": "^3.0.0-beta.6",
    "@vue/cli-plugin-unit-jest": "^3.0.0-beta.6",
    "@vue/cli-service": "^3.0.0-beta.6",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/test-utils": "^1.0.0-beta.10",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "eslint": "^6.1.0",
    "eslint-config-esnext": "^4.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-recommended": "^4.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-only-warn": "^1.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "eslint-traverser": "^1.5.2",
    "lint-staged": "^9.2.1",
    "node-sass": "^4.7.2",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^4.38.0"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ],
    "env": {
      "test": {
        "plugins": [
          "babel-plugin-transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "recommended/esnext",
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "env": {
      "browser": true,
      "jest": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "plugins": [
      "vue",
      "only-warn"
    ],
    "rules": {
      "vue/max-attributes-per-line": 0
    },
    "globals": {
      "process": true
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "roots": [
      "src"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
