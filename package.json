{
  "name": "projectname",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --open",
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "test": "vue-cli-service test --testMatch='<rootDir>/src/**.spec.js'",
    "e2e": "vue-cli-service e2e",
    "lint": "vue-cli-service lint",
    "e2e:open": "vue-cli-service e2e:open"
  },
  "dependencies": {
    "register-service-worker": "^1.0.0",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.6",
    "@vue/cli-plugin-e2e-cypress": "^3.0.0-beta.6",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.6",
    "@vue/cli-plugin-pwa": "^3.0.0-beta.6",
    "@vue/cli-plugin-unit-jest": "^3.0.0-beta.6",
    "@vue/cli-service": "^3.0.0-beta.6",
    "@vue/eslint-config-prettier": "^3.0.0-beta.6",
    "@vue/test-utils": "^1.0.0-beta.10",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^22.0.4",
    "eslint": "^4.18.2",
    "eslint-config-esnext": "^2.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-recommended": "^2.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-only-warn": "^1.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-vue": "^4.3.0",
    "lint-staged": "^6.0.0",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": ["@vue/app"],
    "env": {
      "test": {
        "plugins": ["babel-plugin-transform-es2015-modules-commonjs"]
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": ["recommended/esnext", "plugin:vue/essential", "@vue/prettier"],
    "env": {
      "browser": true,
      "jest": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "plugins": ["vue", "only-warn"],
    "rules": {
      "vue/max-attributes-per-line": 0
    },
    "globals": {
      "process": true
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": ["> 1%", "last 2 versions", "not ie <= 8"],
  "jest": {
    "moduleFileExtensions": ["js", "jsx", "json", "vue"],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": ["jest-serializer-vue"],
    "roots": ["src"]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": ["vue-cli-service lint", "git add"],
    "*.vue": ["vue-cli-service lint", "git add"]
  }
}
